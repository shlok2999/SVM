{
  "env-name": "fjasldf",
  "version": "1",
  "isTemplate": 0,
  "os": "ubuntu",
  "languages": [{
    "language-name":"python",
    "libraries":["numpy"]
    }
  ],
  "resources": {
    "ram": "1G",
    "cpu": "1.0",
    "gpu": "1",
  },
  "datasets": [
    {
      "category-name": "ffs",
      "db": [
        {
          "db-name": "fsgr",
          "dataset": [
            {
              "dataset-name": "effr",
              "version": "34"
            }
          ]
        }
      ]
    }
  ],

  "port-publish": [
    {
      "external": 
      {
        "ports": "9000"
      },
      "internal": {
        "ports": "9000",
        "protocol": "tcp"
      }
    },
    {
      "external": 
      {
        "ports": "9080"
      },
      "internal": {
        "ports": "980",
        "protocol": "udp"
      }
    }
  ],

  "storage": [{
    "target": "/target",
    "size": "104857600",
    "lifecycle": "temporary"
  }]
}